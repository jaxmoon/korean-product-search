{
  "extends": "../backend/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2021",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@elastic/elasticsearch": ["../backend/node_modules/@elastic/elasticsearch"]
    }
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
